@model WebApp.Models.Student

@{
    ViewData["Title"] = "課表";
}



@{
    string[,] courseAry = new string[9, 6]; //week,time
    string[,] TeacherAry = new string[9, 6]; //week,time
    int numtime = 0;
    int numweek = 0;
    foreach (var item in Model.Enrollment)
    {
        {
            string week = item.Teach.Rating;
            char[] time = item.Teach.ClassDate.ToCharArray();

            switch (week)
            {
                case "星期一":
                    numweek = 1;
                    break;
                case "星期二":
                    numweek = 2;
                    break;
                case "星期三":
                    numweek = 3;
                    break;
                case "星期四":
                    numweek = 4;
                    break;
                case "星期五":
                    numweek = 5;
                    break;
            }
            foreach (var itemtime in time)
            {
                switch (itemtime)
                {
                    case 'A':
                        numtime = 1;
                        break;
                    case 'B':
                        numtime = 2;
                        break;
                    case 'C':
                        numtime = 3;
                        break;
                    case 'D':
                        numtime = 4;
                        break;
                    case 'E':
                        numtime = 5;
                        break;
                    case 'F':
                        numtime = 6;
                        break;
                    case 'G':
                        numtime = 7;
                        break;
                    case 'H':
                        numtime = 8;
                        break;
                }
                courseAry[numtime, numweek] = item.Teach.Course;
                TeacherAry[numtime, numweek] = item.Teach.Name;
            }
        }
    }
}

<h1>選課名單</h1>
<dd class="col-sm-10 container">
    <table class="table table-bordered text-center">


        <tr>
            <td>
                課程
            </td>
